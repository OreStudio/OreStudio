{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "name": "ores",
    "description": "GUI for Open Source Risk Engine (ORE)",
    "version-string": "0.0.8",
    "dependencies": [
        "openssl",
        {
            "name": "reflectcpp",
            "features": [ "xml", "bson" ]
        },
        {
            "name": "sqlgen",
            "features": [ "postgres" ]
        },
        "pugixml",
        "magic-enum",
        "boost-system",
        "boost-exception",
        "boost-log",
        "boost-program-options",
        "boost-uuid",
        "boost-crc",
        "boost-scope-exit",
        "boost-process",
        "boost-iostreams",
        "boost-beast",
        "boost-json",
        "boost-url",
        "jwt-cpp",
        "cli",
        "faker-cxx",
        "catch2",
        "libfort",
        "libharu"
    ],
    "default-features": [ "wt" ],
    "features": {
        "wt": {
            "description": "Build the Wt web interface",
            "dependencies": [ "wt" ]
        }
    }
}
