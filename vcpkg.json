{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "name": "ores",
    "description": "GUI for Open Source Risk Engine (ORE)",
    "version-string": "0.0.2",
    "dependencies": [
        "openssl",
        "libpqxx",
        "libbson",
        {
            "name": "reflectcpp",
            "features": [ "xml", "bson" ]
        },
        {
            "name": "sqlgen",
            "features": [ "postgres" ]
        },
        "pugixml",
        "magic-enum",
        "boost-system",
        "boost-date-time",
        "boost-log",
        "boost-filesystem",
        "boost-program-options",
        "boost-test",
        "boost-uuid",
        "boost-crc",
        "cli"
    ]
}
