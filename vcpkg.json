{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "name": "ores",
    "description": "GUI for Open Source Risk Engine (ORE)",
    "version-string": "0.0.2",
    "dependencies": [
        "qtbase",
        "openssl",
        {
            "name": "reflectcpp",
            "features": [ "xml", "bson" ]
        },
        {
            "name": "sqlgen",
            "features": [ "postgres" ]
        },
        "pugixml",
        "magic-enum",
        "boost-system",
        "boost-exception",
        "boost-log",
        "boost-program-options",
        "boost-uuid",
        "boost-crc",
        "boost-scope-exit",
        "cli",
        "faker-cxx",
        "catch2"
    ]
}
